{
    "name": "ferodss/salesforce-api",
    "type": "library",
    "description": "Salesforce API",
    "homepage": "https://github.com/ferodss/salesforce-api",
    "keywords": ["salesforce", "api"],
    "license": "MIT",
    "authors": [{
        "name": "Felipe Rodrigues",
        "homepage": "http://ferodss.github.io"
    }],
    "require": {
        "php": ">=5.3.3",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "~3.7",
        "symfony/event-dispatcher": "~2.5",
        "psr/log": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*"
    },
    "autoload": {
        "psr-4": {
            "Salesforce\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Salesforce\\Tests\\": "tests/"
        }
    }
}
